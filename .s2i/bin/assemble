echo 'cat /etc/passwd'
cat /etc/passwd
echo 'id'
id

export BOOTSTRAP_DOWNLOAD_URL="https://gitweb.gentoo.org/repo/proj/prefix.git/plain/scripts/bootstrap-prefix.sh"


echo 'cd $HOME && pwd && ls -la'
cd $HOME
pwd
ls -la

export USER=user
export USERNAME=user
export LOGNAME=user

echo 'echo $USER'
echo $USER
echo 'echo $USERNAME'
echo $USERNAME
echo 'echo $LOGNAME'
echo $LOGNAME


echo 'wget $BOOTSTRAP_DOWNLOAD_URL'
wget $BOOTSTRAP_DOWNLOAD_URL
mkdir gentoo
chmod +x ./bootstrap-prefix.sh 
printf 'Y\n2\n/home/user/gentoo\nluck\n' | $HOME/bootstrap-prefix.sh


echo 'ls -la'
ls -la

echo '$HOME:'
cd $HOME
pwd
cd ..
ls -la
